{
  "buildCommand": "bunx nx build slack-oauth-backend",
  "outputDirectory": "dist/apps/slack-oauth-backend",
  "framework": null,
  "functions": {
    "apps/slack-oauth-backend/api/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}

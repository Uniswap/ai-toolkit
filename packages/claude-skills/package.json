{
  "name": "@ai-toolkit/claude-skills",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/claude-skills/dist",
          "main": "packages/claude-skills/src/index.ts",
          "tsConfig": "packages/claude-skills/tsconfig.lib.json",
          "assets": [
            {
              "input": "packages/claude-skills/src",
              "glob": "**/*.md",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@ai-toolkit/utils": "0.2.12-next.7"
  }
}

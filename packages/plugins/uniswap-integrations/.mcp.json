{
  "mcpServers": {
    "spec-workflow": {
      "type": "stdio",
      "command": "npx",
      "args": ["@uniswap/spec-workflow-mcp@latest", "--AutoStartDashboard"],
      "env": {}
    },
    "nx-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "nx-mcp@latest"],
      "env": {}
    },
    "notion": {
      "type": "http",
      "url": "https://mcp.notion.com/mcp"
    },
    "linear": {
      "type": "http",
      "url": "https://mcp.linear.app/mcp"
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"],
      "env": {}
    }
  }
}
